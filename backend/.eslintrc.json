{
  "env": {
    "es2017": true,
    "node": true,
    "jest": true
  },
  "extends": ["standard", "prettier"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "no-useless-constructor": "off",
    "no-new": "off",
    "camelcase": "off",
    "quotes": ["error", "single"]
  }
}
